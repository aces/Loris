namespace LORIS\api;

/**
 * A VisitDicomRow is a single dicom archive
 *
 * @category API
 * @package  Loris
 * @author   Xavier Lecours Boucher <xavier.lecours@mcin.ca>
 * @license  http://www.gnu.org/licenses/gpl-3.0.txt GPLv3
 * @link     https://www.github.com/aces/Loris/
 */
class VisitDicomRow implements \LORIS\Data\DataInstance
{
    protected $DBRow;

    /**
     * Create a new CandidatesRow.
     *
     * @param array $row An array of that tarchive properties
     */
    public function __construct(array $row)
    {
        $this->DBRow = $row;
    }

    /**
     * Implements \LORIS\Data\DataInstance interface for this row.
     *
     * @return string the row data.
     */
    public function toJSON() : string
    {
        return json_encode($this->DBRow);
    }
}

